{
    "name": "opengov/og",
    "description": "Project template for Drupal 8 Open Government project with composer",
    "type": "drupal-profile",
    "license": "GPL-2.0-or-later",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type"    : "package",
            "package" : {
                "name"    : "wet-boew/wet-boew",
                "version" : "v4.0.43",
                "type"    : "drupal-library",
                "dist"    : {
                    "url"  : "https://github.com/wet-boew/wet-boew-cdn/archive/v4.0.43.tar.gz",
                    "type" : "tar"
                },
                "source"  : {
                    "url"       : "https://github.com/wet-boew/wet-boew-cdn.git",
                    "type"      : "git",
                    "reference" : "v4.0.43"
                }
            }
        },
        {
            "type"    : "package",
            "package" : {
                "name"    : "wet-boew/theme-gcweb",
                "version" : "v9.4.0",
                "type"    : "drupal-library",
                "dist"    : {
                    "url"  : "https://github.com/wet-boew/themes-cdn/archive/v9.4.0-gcweb.tar.gz",
                    "type" : "tar"
                },
                "source"  : {
                    "url"       : "https://github.com/wet-boew/themes-cdn.git",
                    "type"      : "git",
                    "reference" : "v9.4.0-gcweb"
                }
            }
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "drupal/core-composer-scaffold": "^8.8.0",
        "drupal/admin_toolbar": "3.1.0",
        "drupal/autologout": "1.3.0",
        "drupal/bootstrap_layouts": "5.2.0",
        "drupal/ckeditor_codemirror": "2.3.0",
        "drupal/console": "1.9.8",
        "drupal/core": "8.9.20",
        "drupal/csv_serialization": "2.0.0",
        "drupal/facets": "1.8.0",
        "drupal/fontawesome": "2.21.0",
        "drupal/google_analytics": "3.1.0",
        "drupal/honeypot": "2.0.2",
        "drupal/menu_breadcrumb": "1.16.0",
        "drupal/metatag": "1.16.0",
        "drupal/password_policy": "3.1.0",
        "drupal/pathauto": "1.9.0",
        "drupal/redirect": "1.7.0",
        "drupal/redis": "1.5",
        "drupal/search_api": "1.23.0",
        "drupal/search_api_solr": "4.2.1",
        "drupal/simple_sitemap": "4.1.1",
        "drupal/token_filter": "1.3.0",
        "drupal/views_bootstrap": "4.3.0",
        "drupal/webform": "6.1.3",
        "drush/drush": "^10.0.0",
        "fortawesome/font-awesome": "6.1.1",
        "oomphinc/composer-installers-extender": "^2.0.0",
        "opengov/gcweb": "dev-master",
        "symfony/event-dispatcher": "4.3.4 as 3.4.99",
        "webmozart/path-util": "^2.3",
        "wikimedia/composer-merge-plugin": "2.0.1",
        "zaporylie/composer-drupal-optimizations": "^1.2.0"
    },
    "require-dev": {
        "drupal/core-dev": "^8.9.20"
    },
    "conflict": {
        "drupal/drupal": "*",
        "symfony/http-foundation": "3.4.35",
        "drush/drush": "<8.1.10"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "enable-patching": true,
        "patches": {
            "drupal/views_bootstrap": {
                "Setting \"columns\" is missing on buildOptionsForm": "https://www.drupal.org/files/issues/2019-02-19/setting-columns-is-missing-on-buildOptionsForm-3034141-1.patch"
            },
            "drupal/core": {
                "Submitting a comment in the non default language redirects you to the default language": "https://www.drupal.org/files/issues/2021-11-29/2751269-106.patch",
                "Comments created in translation are displayed only for admin role": "https://www.drupal.org/files/issues/2020-10-27/2958935-31.patch",
                "Allow exposed form for non-ajax block displays": "https://www.drupal.org/files/issues/2020-09-21/2692297-23.patch",
                "Error: Call to a member function getLabel() after enable layout_builder": "https://www.drupal.org/files/issues/2020-04-08/2985882-field-85.patch",
                "Views exposed sort filters does not sort with facets": "https://www.drupal.org/files/issues/2018-10-30/views-exposed-sort-filters-2992672-10.patch"
            }
        },
        "installer-types": [
            "npm-asset",
            "bower-asset"
        ],
        "installer-paths": {
            "html/core": [ "type:drupal-core" ],
            "html/libraries/{$name}": [
                "type:drupal-library",
                "type:npm-asset",
                "type:bower-asset"
            ],
            "html/modules/contrib/{$name}": [ "type:drupal-module" ],
            "html/profiles/{$name}": [ "type:drupal-profile" ],
            "html/themes/custom/{$name}": [ "opengov/gcweb" ],
            "html/themes/contrib/{$name}": [ "type:drupal-theme" ],
            "drush/Commands/{$name}": [ "type:drupal-drush" ]
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "html/"
            }
        },
        "merge-plugin": {
            "include": [
                "html/modules/contrib/ckeditor_codemirror/composer.libraries.json"
            ]
        }
    }
}
